{
  "containerDefinitions": [
    {
      "name": "linkerd",
      "image": "docker.io/buoyantio/linkerd:1.3.6",
      "command": [
        "-log.level=DEBUG",
        "/etc/linkerd/linkerd.yaml"
      ],
      "memory": 1024,
      "portMappings": [
        {
          "hostPort": 4140,
          "containerPort": 4140,
          "protocol": "tcp"
        },
        {
          "hostPort": 4141,
          "containerPort": 4141,
          "protocol": "tcp"
        },
        {
          "hostPort": 9990,
          "containerPort": 9990,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "mountPoints": [
        {
          "containerPath": "/etc/linkerd",
          "sourceVolume": "linkerd-config",
          "readOnly": true
        }
      ],
      "environment": [
        {
          "name": "SERVICE_9990_NAME",
          "value": "linkerd"
        }
      ]
    }
  ],
  "volumes": [
    {
      "host": {
        "sourcePath": "/etc/linkerd"
      },
      "name": "linkerd-config"
    }
  ],
  "family": "linkerd"
}
